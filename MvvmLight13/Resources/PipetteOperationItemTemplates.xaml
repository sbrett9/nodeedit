<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:MvvmLight13.Controls"
                    xmlns:local="clr-namespace:MvvmLight13.Resources"
                    xmlns:selectors="clr-namespace:MvvmLight13.Selectors"
                    xmlns:viewModel="clr-namespace:MvvmLight13.ViewModel">


    <DataTemplate x:Key="aspirateOperationTemplate" DataType="{x:Type viewModel:AspirateOperationViewModel}">
        <controls:OperationControl DataContext="{Binding}" />
    </DataTemplate>

    <selectors:OperationItemTemplateSelector x:Key="operationItemTemplateSelector" AspirateOperationTemplate="{StaticResource aspirateOperationTemplate}" />

</ResourceDictionary>